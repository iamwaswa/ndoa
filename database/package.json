{
  "name": "database",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "author": "Waswa Olunga <olungaw@gmail.com>",
  "scripts": {
    "start": "sanity start",
    "graphql": "sanity graphql deploy --dataset production --playground --generation gen3",
    "deploy:graphql": "npm run graphql",
    "deploy:sanity": "sanity deploy",
    "deploy": "run-p deploy:**",
    "gen:types": "npx sanity-codegen && cd ../client && prettier --write types/database/index.ts && cd ../database",
    "install": "sanity install",
    "undeploy": "sanity undeploy",
    "test": "sanity check",
    "upgrade": "sanity upgrade",
    "prettier": "prettier --write ./**/*.{ts,tsx}",
    "lint": "eslint ./**/*.{ts,tsx}",
    "lint:fix": "eslint ./**/*.{ts,tsx} --fix",
    "format": "npm run prettier && npm run lint:fix"
  },
  "dependencies": {
    "@sanity/base": "^2.2.5",
    "@sanity/components": "^2.1.4",
    "@sanity/core": "^2.2.3",
    "@sanity/dashboard": "^2.1.4",
    "@sanity/default-layout": "^2.2.5",
    "@sanity/default-login": "^2.1.4",
    "@sanity/desk-tool": "^2.2.5",
    "@sanity/vision": "^2.1.4",
    "prop-types": "^15.7",
    "react": "^17.0",
    "react-dom": "^17.0",
    "react-icons": "^4.1.0",
    "sanity-plugin-asset-source-unsplash": "^0.1.3",
    "sanity-plugin-dashboard-widget-vercel": "^0.1.5",
    "sanity-plugin-media": "^1.0.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-icons": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "sanity-codegen": "^0.8.3",
    "typescript": "^4.1.3"
  }
}
